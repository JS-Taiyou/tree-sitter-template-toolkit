{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "template_toolkit",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "content"
        },
        {
          "type": "SYMBOL",
          "name": "directive"
        }
      ]
    },
    "content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^\\[]+"
        },
        {
          "type": "PATTERN",
          "value": "\\["
        }
      ]
    },
    "directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "conditional_block"
        },
        {
          "type": "SYMBOL",
          "name": "foreach_block"
        },
        {
          "type": "SYMBOL",
          "name": "simple_directive"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    },
    "simple_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "SYMBOL",
          "name": "_statement_list"
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "conditional_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "conditional_start_directive"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "elsif_directive"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "else_directive"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_directive"
        }
      ]
    },
    "foreach_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "foreach_directive"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "SYMBOL",
          "name": "end_directive"
        }
      ]
    },
    "foreach_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "STRING",
          "value": "FOREACH"
        },
        {
          "type": "FIELD",
          "name": "iterator",
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        },
        {
          "type": "FIELD",
          "name": "operator",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "="
              },
              {
                "type": "STRING",
                "value": "IN"
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "list",
          "content": {
            "type": "SYMBOL",
            "name": "_value_expression"
          }
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "conditional_start_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "IF"
            },
            {
              "type": "STRING",
              "value": "UNLESS"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_value_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ";"
                },
                {
                  "type": "SYMBOL",
                  "name": "_statement_list"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "elsif_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "STRING",
          "value": "ELSIF"
        },
        {
          "type": "SYMBOL",
          "name": "_value_expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ";"
                },
                {
                  "type": "SYMBOL",
                  "name": "_statement_list"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "else_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "STRING",
          "value": "ELSE"
        },
        {
          "type": "SYMBOL",
          "name": "_statement_list"
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "end_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%"
        },
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": "%]"
        }
      ]
    },
    "_statement_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_directive_statement"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ";"
              },
              {
                "type": "SYMBOL",
                "name": "_directive_statement"
              }
            ]
          }
        }
      ]
    },
    "_directive_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "command_expression"
        },
        {
          "type": "SYMBOL",
          "name": "assignment_expression"
        },
        {
          "type": "SYMBOL",
          "name": "_value_expression"
        }
      ]
    },
    "_value_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ternary_expression"
        },
        {
          "type": "SYMBOL",
          "name": "binary_expression"
        },
        {
          "type": "SYMBOL",
          "name": "filter_expression"
        },
        {
          "type": "SYMBOL",
          "name": "call_expression"
        },
        {
          "type": "SYMBOL",
          "name": "primary_expression"
        }
      ]
    },
    "primary_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "bare_string"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "hash"
        },
        {
          "type": "SYMBOL",
          "name": "parenthesized_expression"
        }
      ]
    },
    "call_expression": {
      "type": "PREC",
      "value": 10,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "function",
            "content": {
              "type": "SYMBOL",
              "name": "primary_expression"
            }
          },
          {
            "type": "FIELD",
            "name": "arguments",
            "content": {
              "type": "SYMBOL",
              "name": "argument_list"
            }
          }
        ]
      }
    },
    "argument_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_value_expression"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "ternary_expression": {
      "type": "PREC_RIGHT",
      "value": -1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "condition",
            "content": {
              "type": "SYMBOL",
              "name": "_value_expression"
            }
          },
          {
            "type": "STRING",
            "value": "?"
          },
          {
            "type": "FIELD",
            "name": "if_true",
            "content": {
              "type": "SYMBOL",
              "name": "_value_expression"
            }
          },
          {
            "type": "STRING",
            "value": ":"
          },
          {
            "type": "FIELD",
            "name": "if_false",
            "content": {
              "type": "SYMBOL",
              "name": "_value_expression"
            }
          }
        ]
      }
    },
    "parenthesized_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_statement_list"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "command_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_value_expression"
          }
        }
      ]
    },
    "keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "INCLUDE"
        },
        {
          "type": "STRING",
          "value": "USE"
        },
        {
          "type": "STRING",
          "value": "SET"
        },
        {
          "type": "STRING",
          "value": "GET"
        },
        {
          "type": "STRING",
          "value": "CALL"
        },
        {
          "type": "STRING",
          "value": "NEXT"
        }
      ]
    },
    "binary_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 3,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              },
              {
                "type": "FIELD",
                "name": "operator",
                "content": {
                  "type": "STRING",
                  "value": "_"
                }
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              },
              {
                "type": "FIELD",
                "name": "operator",
                "content": {
                  "type": "SYMBOL",
                  "name": "logical_op_high"
                }
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              },
              {
                "type": "FIELD",
                "name": "operator",
                "content": {
                  "type": "SYMBOL",
                  "name": "logical_op_low"
                }
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              }
            ]
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 0,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "left",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              },
              {
                "type": "FIELD",
                "name": "operator",
                "content": {
                  "type": "SYMBOL",
                  "name": "comparison_operator"
                }
              },
              {
                "type": "FIELD",
                "name": "right",
                "content": {
                  "type": "SYMBOL",
                  "name": "_value_expression"
                }
              }
            ]
          }
        }
      ]
    },
    "assignment_expression": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "variable"
          },
          {
            "type": "STRING",
            "value": "="
          },
          {
            "type": "SYMBOL",
            "name": "_value_expression"
          }
        ]
      }
    },
    "filter_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_value_expression"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "PATTERN",
          "value": "[^']*"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "hash": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "hash_pair"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "hash_pair"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "hash_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "identifier"
              },
              {
                "type": "SYMBOL",
                "name": "string"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_value_expression"
          }
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "."
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_][a-zA-Z0-9_]*"
    },
    "bare_string": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9_.\\/]+"
    },
    "comparison_operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "=="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "STRING",
          "value": ">="
        }
      ]
    },
    "logical_op_high": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "&&"
        },
        {
          "type": "STRING",
          "value": "and"
        }
      ]
    },
    "logical_op_low": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "||"
        },
        {
          "type": "STRING",
          "value": "or"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}