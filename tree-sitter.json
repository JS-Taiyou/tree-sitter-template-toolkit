{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "template_toolkit",
      "camelcase": "TemplateToolkit",
      "title": "TemplateToolkit",
      "scope": "source.template_toolkit",
      "file-types": [
        "tt2",
        "tt"
      ],
      "injection-regex": "^template_toolkit$",
      "class-name": "TreeSitterTemplateToolkit"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "TemplateToolkit grammar for tree-sitter",
    "authors": [
      {
        "name": "Jorge Solis",
        "email": "jpsb23@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/JS-Taiyou/tree-sitter-template-toolkit"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}